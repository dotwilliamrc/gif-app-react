{
  "extends": [
    "standard", 
    "standard-jsx",
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "key-spacing": ["error", { "beforeColon": false, "afterColon": true}],
    "react/jsx-curly-spacing": ["error", {
      "when": "always",
      "children": {
        "when": "always"
      }
    }]
  }
}
